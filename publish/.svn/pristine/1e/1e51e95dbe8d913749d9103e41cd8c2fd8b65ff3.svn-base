	<script type="text/javascript">
		function modalLayer(){
			var $btn_modal = null;
			var $btn_modal_close = null;
			var $modal_wrap = null;
			var selected_modal = null;
			function init(){
				$btn_modal = $(".modal-open");
				$btn_modal_close = $(".modal-close");
				$modal_wrap = $(".modal-background");
			}
			function initEvent(){
				$btn_modal.on("click", function(){
					selected_modal = $(this).data("modal");
					$("[data-modal-target="+selected_modal+"]").addClass("on");
				});
				$btn_modal_close.on("click", function(){
					$("[data-modal-target="+selected_modal+"]").removeClass("on");
				});
				$modal_wrap.on("click", function(e){
					$(this).removeClass("on");
				});
				$(".modal-wrap").on("click", function(e){
					e.stopPropagation();
				});
			}
			init();
			initEvent();
		}
		$(function() {
			$(".ly_close").addClass('modal-close');
			modalLayer();
			var listLength = $(".tc_wrap tr").length;
			console.log(listLength);
		});
	</script>
