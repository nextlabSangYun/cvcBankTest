.g_table04 td.r_btn {
	position: relative;
	padding-right: 80px;
}
.g_table04 td.r_btn textarea { height: 60px; }
.g_table04 td.r_btn button {
	position: absolute;
	top: 7px;
	right: 0;
	min-width: 70px;
	height: 60px;
	line-height: 60px;
}
.g_table04 + .g_table { margin-top: 20px; }
.g_table04 .sel_target .sel_sort { margin-bottom: 5px; }
.g_table04 .sel_target select { width: 120px; }
.g_table04 .add_case {
	position: relative;
	padding-left: 80px;
}
.g_table04 .add_case button {
	position: absolute;
	top: 9px;
	left: 10px;
	min-width: 60px;
}
.g_table04 .add_case .reg_list { padding: 4px 4px 5px; }
.g_table04 .sel_target02 { position: relative; }
.g_table04 .sel_target02 .sel_sort { margin-bottom: 5px; }
.g_table04 .sel_target02 select { width: 120px; }
.g_table04 .sel_target02 .reg_list { width: calc(100% - 100px); }
.g_table04 .sel_target02 .reg_list + button {
	position: absolute;
	bottom: 5px;
	right: 0;
}
.g_table04 + .tb_desc { margin-top: 13px; }
/*********************************************************************
| 의사결정지원 |
*********************************************************************/
.tit_wrap { position: relative; }
.tit_wrap .btn_wrap {
	position: absolute;
	top: -5px;
	right: 0;
}
.issue_list { margin: 18px 0; }
.list_title {
	position: relative;
	height: 40px;
	border-top-left-radius: 3px;
	border-top-right-radius: 3px;
	background-color: #6570b0;
}
 .list_title p {
 	padding: 12px 17px;
 	font-size: 15px;
	font-weight: bold;
 	color: #fff;
}
 .list_title button {
	position: absolute;
	top: 50%;
	right: 15px;
	transform: translateY(-50%);
	display: block;
	width: 16px;
	height: 16px;
	background: url(../images/common/btn_ly_pop_cls_w.png) center center no-repeat;
}
.table_wrap {
	position: relative;
	height: 100%;
	border-right: 1px solid #dadada;
	border-bottom: 2px solid #6570b0;
	background-color: #fff;
}
.table_wrap:after {
	content: '';
	display: block;
	float: none;
	clear: both;
}
.table_wrap .g_table04 {
	float: left;
	width: calc( 100% - 360px);
	box-sizing: border-box;
}
.table_wrap .g_table04 th { padding: 13px 15px; }
/* border 예외 */
.table_wrap .g_table04 tr:first-child th,
.table_wrap .g_table04 tr:first-child td { border-top: none; }
.table_wrap .g_table04 tr:last-child th,
.table_wrap .g_table04 tr:last-child td { border-bottom: none; }
/* ----------------------------------------------- */
.g_table04 .tb_btn02_home { margin: 2px 4px 2px 0; }
.inc_file li {
	display: inline-block;
	margin: 3px 3px 3px 0;
	border-right: 1px solid #5d68ad;
}
.inc_file li a {
	margin-left: 5px;
	margin-right: 12px;
	color: #5d68ad;
	text-decoration: underline;
}
.inc_file li:last-child { border-right: none; }
.table_wrap .vote_result {
	float: right;
	width: 360px;
	padding: 0 20px;
	box-sizing: border-box;
	text-align: center;
}
.table_wrap .chart{
	position: absolute;
	top: 50%;
	right: 3px;
	transform: translateY(-50%);
	width: 352px;
	text-align: center;
}
.table_wrap .vote_result .th_title {
	text-align: left;
	font-size: 13px;
	font-weight: bold;
}
.back_circle { padding: 20px 0; }
.back_circle:after {
	content: '';
	display: block;
	float: none;
	clear: both;
}
.back_circle div {
	float: left;
	margin-right: 8px;
	width: 100px;
	height: 100px;
	border-radius: 50px;
	background-color: #f3f3f3;
}
.back_circle div:last-child { margin-right: 0; }
.back_circle .vote_num {
	margin: 21px 0 10px 0;
	font-size: 38px;
	font-weight: bold;
}
.back_circle p {
	line-height: 1;
	text-align: center;
}
.issue_list .g_table tr:last-child td { border-bottom: 2px solid #6570b0; }
.g_table04 .back_circle { padding: 30px 0; }
.reg_list.border_none {
	padding: 2px 0;
	border: none;
}
.reg_list.border_none li { margin: 2px 5px 2px 0; }
.g_table04 .reg_button { padding: 5px 12px; }
.vote_none {
	margin-top: 150px;
	text-align: center;
}
.vote_none p {
	padding: 205px 0 22px 0;
	font-size: 18px;
	background: url(../images/bmt/vote_none.png) no-repeat top right 360px;
}