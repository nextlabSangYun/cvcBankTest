/* 달력 */
input[type="text"].input_date {padding-right:38px;background:#fff url('../images/common/ico_calendar.png') no-repeat center right 8px;}
input[type="text"].input_date:disabled {background-color:#dadada;;}
.ui-datepicker-trigger {display:none !important;}


/* 달력 */
.ui-datepicker {position:absolute;left:-9999px;top:-9999px;width:275px;padding-bottom:5px;box-shadow:3px 3px 20px #dadada;border-radius:0 0 3px 3px;background:#fff;}
.ui-datepicker .ui-datepicker-header {position:relative;padding:10px 40px;text-align:center;border-radius:3px 3px 0 0;background:#2f3e9e;}
.ui-datepicker-prev {position:absolute;left:15px;top:50%;margin-top:-6px;}
.ui-datepicker-prev span {display:inline-block;width:12px;height:12px;overflow:hidden;text-indent:-9999px;background:url('../images/common/btn_cal_prev.png') no-repeat ;}
.ui-datepicker-next {position:absolute;right:15px;top:50%;margin-top:-6px;}
.ui-datepicker-next span {display:inline-block;width:12px;height:12px;overflow:hidden;text-indent:-9999px;background:url('../images/common/btn_cal_next.png') no-repeat ;}
.ui-datepicker-title {display:inline-block;}
.ui-datepicker .ui-datepicker-header select {padding:0 10px;width:65px;min-width:65px;height:24px;margin:0 10px;border:0;background:url('../images/common/arr_cal_sel.png') no-repeat 100% 50%;background-size:8px;font-weight:bold;color:#fff;}
.ui-datepicker .ui-datepicker-header select option {color:#000;}
.ui-datepicker table {width:246px;margin: 0 auto;}
.ui-datepicker th {padding:8px 0;border-bottom:1px solid #ddd;font-size:13px;text-align:center;}
.ui-datepicker th:first-child {color:#ec7676;}
.ui-datepicker th:last-child {color:#969ed3;}
.ui-datepicker td {padding:3px 0;text-align:center;font-size:13px;}
.ui-datepicker td a {display:inline-block;width:24px;line-height:24px;border-radius:12px;color:#666;}
.ui-datepicker td:first-child a {color:#ec7676;}
.ui-datepicker td:last-child a {color:#969ed3;}
.ui-datepicker .ui-state-active {display:inline-block;width:24px;line-height:24px;border-radius:12px;color:#fff;background:#2f3e9e;}
.ui-datepicker td.ui-datepicker-other-month a {color:#999;}
.ui-datepicker-buttonpane {display:none;}

