.g_btn04 span {display:inline-block;padding-left:24px;background:url('../images/common/ico_excel.png') no-repeat 0 50%;}
.btn_add {display:inline-block;width:52px;height:52px;text-indent:-9999px;background:url('../images/common/btn_add.png') no-repeat 50% 50%;}
