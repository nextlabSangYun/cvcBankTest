.g_tab li a {display:block;height:45px;line-height:45px;color:#666;}
.g_tab li.on a {color:#666;}
.g_tab li.on a:after {content:'';display:block;width:100%;position:absolute;left:0;bottom:0;height:8px;background:url('../images/common/arr_tab_on.png') no-repeat 50% 50%;}
.tab_wrap .tab_con_wrap {border:1px solid #ccc;padding:17px 20px 20px;background:#fff;}
.tab_wrap .tab_con {display:none;}
.tab_wrap .tab_con.on {display:block;}

