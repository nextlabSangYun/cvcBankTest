.pop_cont .area_tit {margin:20px 0 10px;font-size:15px;}
.pop_cont .area_tit:after {content:'';display:inline-block;width:1px;height:10px;margin:0 10px;background:#aaa;}

.file_upload label {display:inline-block;height:30px;padding:0 13px;line-height:30px;border:1px solid #ccc;border-radius:3px;box-sizing:border-box;font-size:12px;color:#666;cursor:pointer;}
.file_upload ul {margin-top:5px;}
.file_upload li {color:#959595;line-height:18px;}
.file_upload .file {margin-top:3px;}
.file_upload .file a {color:#666;text-decoration:underline}
.file_upload .file .del {margin-left:13px;display:inline-block;overflow:hidden;position:relative;top:-1px;width:12px;height:12px;text-indent:-9999px;background:url('../images/bmt/del_file.png') no-repeat 50% 50%;}

