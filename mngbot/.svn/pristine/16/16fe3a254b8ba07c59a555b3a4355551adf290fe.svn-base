.g_table04 th:first-child {border-top:1px solid #dadada;}
.g_table04 td {padding:13px 15px;text-align:left;border-bottom:1px solid #dadada;border-right:1px solid #ccc;}
.g_table04 td.has_btn {position:relative;padding-top :45px;}
.g_table04 tr:first-child td {border-top:1px solid #dadada;}

.tb_btn_area {margin:10px 0;text-align:right;}
.tb_btn_area button {display:inline-block;height:35px;padding:0 13px;line-height:35px;border:1px solid #ccc;border-radius:3px;box-sizing:border-box;font-size:14px;color:#666;}

.tb_desc {position:relative;height:30px;margin-bottom:14px;color:#444;}
.tb_desc.r_align {text-align:right;}
.tb_desc p {padding-top:15px;}
.tb_desc [class^="g_btn"] {position:absolute;right:0;top:0;}
.tb_desc .btn_wrap {position:absolute;right:0;top:0;}
.tb_desc .btn_wrap [class^="g_btn"] {position:static;margin-left:10px;}
.tb_desc .btn_wrapm [class^="g_btn"] {position:static;float:right;margin-left:10px;}
.tb_desc .sort_tit:after {content:'';display:inline-block;width:1px;height:9px;margin:0 15px;background:#333;}
.g_table02 .tb_desc {margin:10px 0 0;height:auto;}

.pagination {text-align:center;padding:30px 0 20px;}
.pagination ol {display:inline-block;}
.pagination ol:after {content:'';display:block;clear:both;}
.pagination li {float:left;margin:0 5px;line-height:20px;text-align:center;}
.pagination li button {overflow:hidden;width:20px;height:20px;font-size:12px;color:#aaa;}
.pagination .btn_first button {text-indent:-9999px;background:url('../images/common/btn_first.png') no-repeat 50% 50%}
.pagination .btn_prev button {text-indent:-9999px;background:url('../images/common/btn_prev.png') no-repeat 50% 50%}
.pagination .btn_next button {text-indent:-9999px;background:url('../images/common/btn_next.png') no-repeat 50% 50%}
.pagination .btn_last button {text-indent:-9999px;background:url('../images/common/btn_last.png') no-repeat 50% 50%}
.pagination li.on button,
.pagination li.num button:hover {background-color:#2f3e9e;color:#fff;}
.tab_con .pagination {padding-bottom:0 !important;}

.bg_dim {display:none;z-index:600;position:fixed;left:0;top:0;width:100%;height:100%;background:url('../images/common/bg_dim.png') repeat 0 0;}
.popLayer {display:none;z-index:700;position:absolute;top:50%;left:50%;opacity:0;padding:10px;border-radius:10px;background:#fff;}
.pop_wrap {position:relative;box-sizing:border-box;}
