.btn_show {display:inline-block;width:20px;height:20px;background:url('../images/common/ico_show.png') no-repeat 0 0;background-size:20px;}

.tb_btn01 {display:inline-block;min-width:60px;height:25px;line-height:25px;padding:0 14px;border-radius:3px;text-align:center;color:#fff;background:#2f3e9e;}
.tb_btn02 {display:inline-block;min-width:60px;height:25px;line-height:25px;padding:0 14px;border-radius:3px;text-align:center;color:#fff;background:#666;}
.tb_btn03 {display:inline-block;min-width:60px;height:25px;line-height:25px;padding:0 14px;border-radius:3px;text-align:center;color:#fff;background:#6570b0;}
.tb_btn04 {display:inline-block;min-width:60px;height:25px;line-height:25px;padding:0 14px;border-radius:3px;text-align:center;color:#fff;background:#ff7403;}

.g_table table {width:100%;table-layout:fixed;background:#fff;}
.g_table th, .g_table td{text-align:center;}
.g_table th {padding:5px 0;font-weight:bold;color:#444;border:solid #dadada;border-width:1px 1px 0 0;background:#e9e9e9;line-height:24px;}
.g_table th:first-child {border-left:1px solid #dadada;}
.g_table td {padding:5px 7px;height:24px;border:solid #dadada;border-width:1px 1px 0 0;line-height:16px;word-wrap:break-word;}
.g_table td:first-child {border-left:1px solid #dadada;}
.g_table tr:last-child td {border-bottom:1px solid #dadada;;}
.g_table td .link {color:#5d68ad;}
.g_table td.inactive {color:#aaa;}
.g_table tr:hover td  {background:#f3f4f9;}
.g_table tr a.link:hover {text-decoration:underline}
.g_table td.l_align {text-align:left;padding:5px 15px;}
.g_table td.r_align {text-align:right;padding:5px 10px;}
.g_table td.c_align {text-align:center;}
.g_table td.bd_btm {border-bottom:1px solid #dadada;}
.g_table td button + button {margin-left:5px;}
.g_table td .ellips {width:100%;}
.g_table tr.selected {background:#f3f4f9;}

.g_table02 {background:#fff;}
.g_table02 table {width:100%;border-bottom:1px solid #dadada;}
.g_table02 th {border-right:1px solid #dadada;padding:14px 15px;text-align:left;border-top:1px solid #dadada;border-left:1px solid #dadada;color:#444;}
