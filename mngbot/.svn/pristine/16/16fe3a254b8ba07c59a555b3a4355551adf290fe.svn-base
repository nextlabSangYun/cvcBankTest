.sort_area {position:relative;margin-bottom:23px;min-height:64px;padding:12px 120px 12px 5px;border:1px solid #ddd;box-sizing:border-box;background:#fff;}
.sort_area select {min-width:100px;max-width:170px;vertical-align:top}
.sort_area select:first-child {margin-right:20px;}
.sort_area > .g_btn01 {position:absolute;right:20px;bottom:17px;}
.sort_area:after {content:'';display:block;clear:both;}
.sort_area .sel_item {float:left;padding:5px 7px;width:50%;height:40px;box-sizing:border-box;}
.sort_area .sel_item.full {width:100%;float:none;clear:both;}
.sort_area .sort_tit {width:135px;margin-left:35px;line-height:30px;display:inline-block;color:#333;text-align:right;vertical-align:top;}
.sort_area .sort_tit:first-child {margin-left:0;}
.sort_area .sort_tit:after {content:'';display:inline-block;width:1px;height:9px;margin:0 15px;background:#333;}
.sort_area .sel_item.full {position:relative;padding:5px 25px 5px 162px;}
.sort_area .sel_item.full .sort_tit {position:absolute;left:25px;top:5px;}
.sort_area label {line-height:30px;}

.g_tab {position:relative;top:1px;}
.g_tab:after {content:'';display:block;clear:both;}
.g_tab li {position:relative;float:left;width:186px;text-align:center;background:#e9e9e9;border:1px solid #ccc;border-left:0;}
.g_tab li.on {border-bottom:1px solid #fff;background:#fff;}
