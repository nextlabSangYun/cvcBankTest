/*프로젝트 overview*/

.text-left { text-align: left !important; }
.text-center { text-align: center !important; }

/* a | a링크 재설정 */
a { color: #666; }
a.project-section-title {
	display: block;
	text-decoration: none;
	cursor: pointer;
}
/* a:hover - active | a링크 호버 시 액션 */
a.project-link:hover span { text-decoration: underline; }
a.project-section-title:hover .pov-dwrap, tr:hover td { background-color: #f3f4f9; }

/* float clear | float 적용 해제 */
.pov-dwrap:after, .project-section-content dl:after, .pov-btn-wrap:after {
	content: '';
	display: block;
	float: none;
	clear: both;
}

.project-link {
	position: absolute;
	top: 13px;
	left: 24px;
	width: 180px;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}

/* --------------------------------------------------------------------- */
.pov-project {
	margin-left: auto;
	margin-right: auto;
}
.pov-project h1 {
	margin-bottom: 20px;
	padding-left: 18px;
	font-size: 17px;
	font-weight: normal;
	color: #444;
	background: url(img/bull_cate_tit1.png) left center no-repeat;
	background-size: 8px;
}
/* border 설정(기본) */
.project-section-title, .pov-tb-group th, .pov-tb-group td { border: 1px solid #dadada; }
/* border 설정(예외) */
.pov-project { position: relative; }
.project-list li:first-child a { border-top: 0; }

.bor-bottom-del .project-section-content { border-bottom: 0; }
.pov-project h2 {
	padding: 12px 0;
	font-size: 13px;
	border: 1px solid #dadada;
	background-color: #e9e9e9;
}
.pov-project ul {
	height: 696px;
	overflow-x: hidden;
	overflow-y: auto;
	background-color: #fff;
	border: 1px solid #dadada;
	border-top: 0;
}
.pov-project li {
	position: relative;
	width: 99.6%;
	margin-left: 1px;
	margin-bottom: -1px;
	background-color: #fff;
}
/* bar default | 리스트 왼쪽 바 레이아웃 */
.pov-bar-default {
	position: absolute;
	top: 1px;
	left: 0;
	z-index: 100;
	width: 8px;
	height: 41px;
}
/* bar color | 리스트 왼쪽 바 색상 */
.pov-bar-seegreen { background-color: #2e8b57; }
.pov-bar-steelblue { background-color: #6495ed; }
.pov-bar-aquamarine { background-color: #40e0d0; }
.pov-bar-crimson { background-color: #dc143c; }
.pov-bar-lavender { background-color: #c2c2e6; }
.pov-bar-coral { background-color: #ff7f50; }
.pov-bar-orange { background-color: #ffa500; }
.pov-bar-plum { background-color: #ee82ee; }
.pov-bar-cyan { background-color: #48d1cc; }
.pov-bar-olive { background-color: #91a835; }

.pov-dwrap {
	padding-left: 18px;
	padding-right: 16px;
}
.pov-dwrap p {
	padding: 12px 0;
	float: left;
	text-align: center;
}

.pov-dwrap p:nth-child(1) { margin-left:200px; width: 17%; }
.pov-dwrap p:nth-child(2) { width: 12%; }
.pov-dwrap p:nth-child(3) { width: 12%; }
.pov-dwrap p:nth-child(4) { width: 13%; }

.pov-ico-arrow {
	position: absolute;
	top: 12px;
	right: 20px;
	z-index: 1000;
}
.project-section-content {
	display: none;
	width: 100%;
	margin-left: 1px;
	border-right: 1px solid #dadada;
	border-bottom: 1px solid #dadada;
}
.project-section-content dl { padding: 20px 10px 15px 10px; }
.project-section-content dt {
	display: block;
	clear: left;
	width: 80px;
	font-weight: bold;
	text-align: center;
	border-right: 1px solid #666;
}
.project-section-content dt, .project-section-content dd {
	float: left;
	margin-top: 5px;
	margin-bottom: 5px;
	line-height: 1.2;
}
.project-section-content dd {
	padding-left: 18px;
	width: 385px;
}
.pov-btn-wrap {
	border: 1px solid #ddd;
	border-radius: 5px;
	padding: 4px;
}
.pov-btn-wrap { line-height: 1.2; }

/* button default | 버튼 레이아웃 */
.pov-btn-default {
	float: left;
	margin: 3px;
	padding: 6px 8px;
	border-radius: 3px;
	font-size: 11px;
	color: #fff;
}
/* button color | 버튼 색상 */
.pov-btn-blue { background-color: #6570b0; }
.pov-btn-grey { background-color: #666; }

.pov-btn-del {
	display: inline-block;
	margin-top: -1px;
	margin-left: 2px;
	width: 14px;
	height: 14px;
	background: url(img/del_reg_list.png) center center no-repeat;
}
.pov-tb-group { width: 100%; }
.pov-tb-group th, .pov-tb-group td {
	padding: 5px 0;
	width: 45%;
	text-align: center;
	line-height: 2;
}
.pov-tb-group th { background-color: #e9e9e9; }