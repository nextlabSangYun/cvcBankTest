/* 파일 업로드 버튼 */
.sch_file {
	display: inline-block;
	height: 30px;
	line-height: 29px;
	padding: 0 6px;
	color: #666;
	text-align: center;
	vertical-align: top;
	border: 1px solid #aaa;
