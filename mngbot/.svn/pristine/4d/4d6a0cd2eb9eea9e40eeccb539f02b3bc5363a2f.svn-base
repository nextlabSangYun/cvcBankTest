.notice li {
	position: relative;
	padding: 1px 0 1px 10px;
	line-height: 1.5;
}
.notice li:before {
	content: '*';
	position: absolute;
	left: 0;
	font-weight: bold;
	color: #6570b0;
}
.warning {
	margin: 10px 0;
	padding: 10px 15px;
	border: 1px dashed #cc0a0a;
	background-color: #f7f0f0;
}
.warning li {
	position: relative;
	padding: 1px 0 1px 22px;
	line-height: 1.5;
	color: #cc0a0a;
}
.warning li:before {
	content: '!';
	position: absolute;
	left: 0;
	display: inline-block;
	vertical-align: middle;
	width: 16px;
	height: 16px;
	border-radius: 8px;
	color: #fff;
	text-align: center;
	background-color: #cc0a0a;
}
