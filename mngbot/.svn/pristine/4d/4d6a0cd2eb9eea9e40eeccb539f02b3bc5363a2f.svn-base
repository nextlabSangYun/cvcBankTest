| 공통스타일 |
*********************************************************************/
.full {width:100% !important;}
.ellips {overflow:hidden;width:99%;text-overflow:ellipsis;white-space:nowrap;}
.point_cr {color:#2f3e9e;}
input[type="radio"], input[type="checkbox"] {position:relative;top:-1px;}
input[type="radio"] + label {display:inline-block;margin:0 15px 0 8px;}
input[type="checkbox"] + label {display:inline-block;margin:0 15px 0 8px; font-weight: normal;}
input[type="text"], input[type="password"] {height:30px;box-sizing:border-box;border:1px solid #ccc;padding:0 7px;color:#666;background:#fff;}
input[type="text"].bg_gr {background:#dadada;}
input[type="text"]:disabled {background:#dadada;}
input[type=text]::-ms-clear { display:none; }
select {height:30px;padding:0 30px 0 7px;min-width:100px;border:1px solid #ccc;box-sizing:border-box;-webkit-appearance:none;-ms-appearance:none;-moz-appearance:none;font-size:12px;color:#666;background:#fff url('../images/common/arr_select.png') no-repeat 100% 50%;}
select::-ms-expand {display: none;}
select[disabled="disabled"] { background: #ebebe4 url(../images/common/arr_select_disabled.png) no-repeat 100% 50%; }
textarea {border:1px solid #ccc;padding:7px;resize:none;box-sizing:border-box;color:#666;font-size:12px;}
textarea.bg_gr {background:#dadada;}

.fl { float: left; }
.fr { float: right; }
.clear:after {
	content: '';
	display: block;
	float: none;
	clear: both;
}
.mgtop40 { margin-top: 40px !important; }
.w100 { width: 100% !important; }
.l_align { text-align: left; }
