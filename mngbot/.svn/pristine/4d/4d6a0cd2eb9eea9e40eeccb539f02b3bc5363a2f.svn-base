@charset 'utf-8';

@import url('https://fonts.googleapis.com/earlyaccess/notosanskr.css');
@import url('https://fonts.googleapis.com/css?family=Roboto');


html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,button,table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,embed,figure,figcaption,footer,header,hgroup,menu,nav,output,ruby,section,summary,time,mark,audio,video {margin: 0;padding: 0}

html,body {width:100%;min-height:100%;height:100%;-webkit-text-size-adjust: none; -webkit-tap-highlight-color: transparent;word-break: keep-all;-webkit-font-smoothing: antialiased;-webkit-user-select:none;-webkit-touch-callout:none;font-family:'Roboto', 'Noto Sans KR', Malgun gothic,'맑은 고딕', '돋움', Dotum, Helvetica, Sans-serif;font-size:12px;line-height:1;color:#666;}
h1,h2,h3,h4,h5,h6 {font-weight: normal;font-size: inherit}
article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section { display: block}
ol,ul {list-style: none}
address,caption,cite,code,dfn,em,th,var{font-style:normal;font-weight:normal;} 
blockquote,q {quotes: none}
blockquote:before,blockquote:after,q:before,q:after {content: '';content: none}
img,fieldset,a { border: 0}
img {vertical-align: middle}
table,thead,tbody,tfoot {border-collapse: collapse;border-spacing: 0;border: 0 none;word-break: keep-all}
table th,table td {word-break: keep-all;}
.tb_break { word-break: break-all !important; }
ul,ol,li {list-style: none;word-break: keep-all}
address,em {font-style: normal}
input {margin:0;}
input,select,textarea,button,form img {vertical-align: middle;font-size:12px;font-family:'Roboto', 'Noto Sans KR', Malgun gothic,'맑은 고딕', '돋움', Dotum, Helvetica, Sans-serif;}
fieldset {min-width: 0}
button,input[type="submit"] {cursor: pointer}
input:focus,button:focus,select:focus,textarea:focus {outline: 0}
button::-moz-focus-inner {border: 0 none;padding: 0}
button {border: 0;border-radius: 0;background:0;font-size:12px;line-height:1;color:#666;}
a {text-decoration: none;cursor:pointer;}
a:focus {;outline:none;}
.ir, caption {display:block;overflow: hidden;position: absolute;top:0;left:0;width: 0;z-index:-1;opacity:0 !important;}

/* 레이아웃 */
body {background:#f3f3f3;}
.inner {max-width:1200px;margin:0 auto;box-sizing:border-box;}
.wrap {position:relative;width:100%;min-width:1200px;min-height:100%;}
header {position:absolute;width:100%;height:54px;left:0;top:0;box-sizing:border-box;background:#2f3e9e;z-index:500;}
header .inner {padding: 0 36px 0 75px;position:relative;max-width:100%;}
header .inner:after {content:'';display:block;clear:both;}
header .btn_nav  {display:inline-block;width:55px;height:54px;position:absolute;left:15px;top:0;text-align:center;}
header .btn_nav .icon {display:inline-block;position:relative;top:-4px;width:27px;height:3px;background:#fff;}
header .btn_nav .icon:before, 
header .btn_nav .icon:after{content:'';display:block;position:absolute;left:0;width:27px;height:3px;background:#fff;transition:all 0.2s ease-out;}
header .btn_nav .icon:before {top:-9px;}
header .btn_nav .icon:after {top:9px;}
.nav_on header .btn_nav .icon {background:transparent;}
.nav_on header .btn_nav .icon:before {top:0;transform-origin:50% 50%;transform:rotate(-45deg);}
.nav_on header .btn_nav .icon:after {top:0;transform-origin:50% 50%;transform:rotate(45deg);}

header  .syt_name {display:inline-block;float:left;line-height:54px;font-weight:bold;font-size:21px;color:#fff;}
header .btn_right {float:right;padding:8px 0 0;}
header .btn_notice {position:relative;width:36px;height:38px;background:url('../images/common/btn_notice.png') no-repeat 50% 100%;color:#fff;}
header .btn_notice:active {background-size:auto 32px;}
header .btn_notice.has_new {background:url('../images/common/btn_notice_new.png') no-repeat 0 100%;}
header .btn_notice.has_new:active {background-size:auto 32px;}
header .btn_notice .new {display:inline-block;position:absolute;right:1px;top:2px;width:16px;height:16px;border-radius:8px;line-height:16px;text-align:center;color:#fff;font-size:10px;background:#ec1b1b;}
header .btn_notice:active .new {width:15px;height:15px;right:2px;top:4px;}
header .btn_setting {margin-left:26px;line-height:37px;font-size:17px;color:#fff;font-family:'Noto Sans KR';}
header .btn_setting:after {content:'';display:block;clear:both;}
header .btn_setting span {float:left;}
header .btn_setting .photo {display:inline-block;width:37px;height:37px;border-radius:19px;margin-right:7px;background-repeat:no-repeat;background-position:50% 50%;background-size:cover;}

.nav_on nav {display:block;}
nav {display:none;z-index:400;position:absolute;top:54px;left:0;width:330px;padding:20px 30px;box-sizing:border-box;background:#fff;}
nav .cate_tit {font-size:19px;color:#2f3e9e;}
nav .cate_tit:after {content:'';display:block;margin-top:10px;width:100%;height:1px;background:#9e9e9e;}
nav .app_list ul {padding:26px 0 18px;}
nav .app_list ul:after {content:'';display:block;clear:both;}
nav .app_list li {position:relative;float:left;width:50%;text-align:center;padding-top:4px;margin-bottom:35px;font-size:14px;}
nav .app_list li a {color:#2a2a2a;}
nav .app_list .app_icon {display:inline-block;position:relative;margin-bottom:16px;}
nav .app_list .app_name {display:block;}
nav .app_list li .btn_fav {content:'';display:inline-block;position:absolute;left:50%;top:-4px;margin-left:24px;width:22px;height:22px;background:url('../images/common/ico_fav.png') no-repeat 0 0;}
nav .app_list li.fav .btn_fav {background-image:url('../images/common/ico_fav_on.png');} 

.ly_notice {display:none;z-index:400;position:absolute;top:54px;right:0;width:330px;padding:20px 30px;box-shadow:-3px 4px 24px #b4b4b4;box-sizing:border-box;background:#fff;}
.ly_notice.show {display:block;}
.ly_notice .cate_tit {font-size:19px;color:#2f3e9e;}
.ly_notice .cate_tit:after {content:'';display:block;margin-top:10px;width:100%;height:1px;background:#9e9e9e;}
.ly_notice .notice_list {position:relative;}
.ly_notice .notice_list:last-child {margin-top:40px;}
.ly_notice .notice_list li {padding:10px 5px;border-bottom:1px solid #dadada;}
.ly_notice .notice_list .tit {overflow:hidden;width:80%;margin-bottom:8px;text-overflow:ellipsis;white-space:nowrap;font-size:17px;line-height:1;}
.ly_notice .notice_list .tit a {color:#252525;}
.ly_notice .notice_list .info {font-size:13px;color:#9f9f9f;}
.ly_notice .notice_list .info:after {content:'';display:block;clear:both;}
.ly_notice .notice_list .info p {display:inline-block;float:left;}
.ly_notice .notice_list .info .writer {float:right;}
.ly_notice .btn_more {display:inline-block;overflow:hidden;width:18px;height:14px;position:absolute;right:0;top:0px;text-indent:-9999px;background:url('../images/common/btn_more_list.png') no-repeat 100% 50%;}

.ly_my_setting {display:none;z-index:500;position:absolute;right:0;top:54px;width:148px;border:1px solid #e5e5e5;border-top:0;padding:15px 10px 18px 14px;box-sizing:border-box;background:#fff;}
.ly_my_setting.show {display:block;}
.ly_my_setting li {margin-bottom:18px;line-height:19px;font-size:14px;}

.ly_my_setting li:last-child {margin-bottom:0;}
.ly_my_setting li a {float:left;color:#777;}
.ly_my_setting li a:before {content:'';display:inline-block;float:left;width:18px;height:19px;margin-right:8px;background:url('../images/common/ico_my_info.png') no-repeat 0 50%;}
.ly_my_setting li.bg a:before {background-image:url('../images/common/ico_bg_chg.png')}
.ly_my_setting li.logout a:before {background-image:url('../images/common/ico_logout.png')}
.ly_my_setting li:after {content:'';display:block;clear:both;}

.pop_notice {z-index:1000;width:400px;overflow:hidden;position:fixed;right:10px;bottom:10px;}
.pop_notice ul {display:flex;flex-direction: column-reverse;}
.pop_notice li {z-index:1000;display:none;overflow:hidden;position:absolute;right:0;margin-top:10px;height:120px;width:400px;bottom:0;}
.pop_notice li.show {display:block;}
.pop_notice .msg_wrap {overflow:hidden;position:absolute;right:0;width:400px;padding:30px 20px 20px 108px;border:1px solid #454545;background:#fff;box-sizing:border-box;bottom:-120px;}
.pop_notice .writer {position:absolute;left:24px;top:20px;width:62px;margin-right:23px;text-align:center;}
.pop_notice .writer a {display:block;width:62px;height:82px;}
.pop_notice .writer .photo {display:inline-block;width:62px;height:62px;border-radius:31px;background-position:50% 50%;background-repeat:no-repeat;background-size:cover;}
.pop_notice .writer .name {display:block;margin-top:5px;color:#242424;font-size:13px;}
.pop_notice .info a {display:block;}
.pop_notice .info .tit {overflow:hidden;width:98%;margin-bottom:12px;text-overflow:ellipsis;white-space:nowrap;font-size:16px;line-height:1;color:#666;}
.pop_notice .info .link {overflow:hidden;text-overflow:ellipsis;display:-webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;word-wrap:break-word;height:40px;line-height:20px;font-size:13px;color:#9f9f9f;}
.pop_notice .info .link a {color:#9f9f9f;}
.pop_notice .btn_cls {display:inline-block;overflow:hidden;width:13px;height:13px;position:absolute;right:11px;top:11px;text-indent:-9999px;background:url('../images/common/btn_pop_notice_cls.png') no-repeat 0 0;}

.content {position:relative;padding-top:89px;box-sizing:border-box;}
.sub_layout {padding:28px 30px;max-width:1200px;margin:5px auto 0 auto;box-sizing:border-box;}

.location {position:absolute;left:0;top:54px;z-index:150;width:100%;height:35px;background:#d6d9ec;}
.location ol {padding:0 30px;box-sizing:border-box;}
.location ol:after {content:'';display:block;clear:both;}
.location ol li {float:left;line-height:35px;color:#555;font-size:14px;}
.location ol li:before {content:'';display:inline-block;width:7px;height:10px;margin:0 16px;background:url('../images/common/arr_cate.png') no-repeat 50% 50%;}
.location ol li:first-child:before {display:none;}
.location ol li a {color:#555;}
.location .app_ico {display:inline-block;position:relative;top:-1px;width:21px;height:21px;margin-right:9px;vertical-align:top;}
.location .app_ico img {width:21px;height:21px;}

