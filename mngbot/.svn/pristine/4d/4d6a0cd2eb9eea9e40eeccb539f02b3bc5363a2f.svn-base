/* table td checkbox -*/
.member { padding: 0 10px; }
.member:after {
	content: '';
	display: block;
	float: none;
	clear: both;
}
.member > span {
	display: block;
	float: left;
	margin-right: 10px;
	padding-top: 5px;
}
.member input[type="checkbox"] { margin: 0; }
/* 진행상황 신규 라벨 */
.newlabel {
	position: relative;
	height: 24px;
}
.newlabel:before {
	position: absolute;
	content: 'N';
	top: 0;
	right: 3px;
	padding: 2px 3px;
	font-size: 9px;
	line-height: 0.8;
	color: #fff;
	text-align: center;
	border-radius: 3px;
	background-color: #f45858;
}